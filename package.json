{
	"name": "js-playground",
	"version": "1.0.0",
	"main": "src/imp.ts",
	"license": "MIT",
	"scripts": {
		"digwatch": "babel-node ./src/domain/dig-watch.js",
		"watch:req": "babel-node ./src/watch/request.js",
		"lint": "eslint --ext .ts,.tsx,.js src --fix",
		"test": "jest",
		"precommit": "lint-staged",
		"free": "jest src/freedom.test.js --watch",
		"phptojs:ei": "babel --no-babelrc --presets php src/binary/ei-decode.php -o src/binary/ei-decode.js",
		"newname:gen": "python2 src/newname/dump_name_cand.py > data/names.json",
		"newname:filter": "babel-node src/newname/index.js > data/names-none.json",
		"newname:comp": "babel-node src/newname/filter-suspended.js > data/names-comp.json",
		"prepare": "husky install"
	},
	"resolutions": {
		"babel-core": "7.0.0-bridge.0"
	},
	"dependencies": {
		"@elzup/kit": "^4.1.1",
		"@makenowjust-labo/redos": "^1.2.0",
		"@octokit/graphql": "^5.0.5",
		"axios": "^0.27.2",
		"base-64": "^1.0.0",
		"canvas": "^2.9.1",
		"cheerio": "^1.0.0-rc.11",
		"cli-table": "^0.3.11",
		"cron-schedule": "^3.0.6",
		"d3-node": "^2.2.3",
		"jaco": "^4.0.0",
		"japanese-holidays": "1.0.10",
		"jest": "^28.1.0",
		"jimp": "^0.16.1",
		"jptext-to-emoji": "^0.1.2",
		"js-vim-commands": "^0.0.0",
		"keypress": "^0.2.1",
		"kuromojin": "^3.0.0",
		"lodash": "^4.17.11",
		"moment": "^2.29.3",
		"node-fetch": "^3.2.4",
		"node-tesseract": "^0.2.7",
		"nominal-types": "0.2.0",
		"sprintf-js": "^1.1.1",
		"tone": "^14.7.77",
		"twitter": "^1.7.1",
		"utf8": "^3.0.0",
		"youtube-node": "^1.3.3"
	},
	"devDependencies": {
		"@babel/core": "^7.18.2",
		"@babel/node": "^7.17.10",
		"@babel/plugin-proposal-pipeline-operator": "^7.18.2",
		"@babel/preset-env": "^7.18.2",
		"@babel/preset-typescript": "^7.17.12",
		"@elzup/tsconfig": "^1.0.2",
		"@swc/core": "^1.2.196",
		"@swc/jest": "^0.2.21",
		"@types/bluebird": "^3.5.36",
		"@types/cheerio": "^0.22.31",
		"@types/cli-table": "^0.3.0",
		"@types/crypto-js": "^4.1.1",
		"@types/japanese-holidays": "^1.0.1",
		"@types/jest": "^27.5.1",
		"@types/lodash": "^4.14.182",
		"@types/node": "^17.0.38",
		"@types/node-fetch": "^2.6.1",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.5",
		"@types/request": "^2.48.8",
		"@types/twitter": "^1.7.1",
		"eslint": "^8.16.0",
		"eslint-config-nzap": "^5.3.1",
		"husky": "^8.0.1",
		"jest-cli": "^28.1.0",
		"lint-staged": "^12.5.0",
		"prettier": "^2.6.2",
		"textlint": "^12.1.1",
