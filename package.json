		"ts-jest": "^28.0.3",
		"ts-node": "^10.8.0",
		"typescript": "4.7.2"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		],
		"*.tsx?": [
			"eslint --fix",
			"git add"
		]
	},
	"ts-node": {
		"transpileOnly": true,
		"transpiler": "ts-node/transpilers/swc-experimental"
	}
}
